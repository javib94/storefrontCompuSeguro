    "@types/graphql": "^14.0.5",
    "@types/jasmine": "~3.5.14",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^14.11.2",
    "@types/photoswipe": "^4.1.0",
    "codelyzer": "^6.0.1",
    "@graphql-codegen/add": "^2.0.1",
    "@graphql-codegen/cli": "^1.13.2",
    "@graphql-codegen/fragment-matcher": "^1.17.8",
