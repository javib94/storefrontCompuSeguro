{
  "name": "vendure-angular-storefront",
  "version": "0.1.24",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201 --host 0.0.0.0",
    "build": "ng build",
    "analyze-prod-bundle": "ng build --prod --statsJson=true && webpack-bundle-analyzer dist/browser/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-types": "gql-gen --config codegen.yml",
    "dev:ssr": "ng run reference-storefront:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run reference-storefront:server:production",
    "build:ssr:docker": "ng build --configuration=production,docker && ng run reference-storefront:server:docker",
    "prerender": "ng run reference-storefront:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.4",
    "@angular/cdk": "^10.2.3",
    "@angular/common": "^10.1.4",
    "@angular/compiler": "^10.1.4",
    "@angular/core": "^10.1.4",
    "@angular/forms": "^10.1.4",
    "@angular/material": "^11.1.1",
    "@angular/platform-browser": "^10.1.4",
    "@angular/platform-browser-dynamic": "^10.1.4",
    "@angular/platform-server": "^10.1.4",
    "@angular/router": "^10.1.4",
    "@angular/service-worker": "^10.1.4",
    "@apollo/client": "^3.0.0",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-free": "~5.6.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.31",
    "@fortawesome/free-brands-svg-icons": "^5.15.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.0",
    "@nguniversal/express-engine": "^10.1.0",
    "@types/chart.js": "^2.7.40",
    "angular-bootstrap-md": "^10.1.1",
    "angular-responsive-carousel": "^2.0.2",
    "animate.css": "~3.7.2",
    "apollo-angular": "^2.0.4",
    "bootstrap": "^4.5.2",
    "chart.js": "^2.7.2",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "express": "^4.15.2",
    "graphql": "^15.0.0",
    "hammerjs": "~2.0.8",
    "photoswipe": "^4.1.3",
    "rxjs": "~6.6.3",
    "sweetalert2": "^10.15.6",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular-devkit/build-ng-packagr": "~0.1001.4",
